@prefix aegis: <http://aegis-bigdata.eu/vocabulary/> .
@prefix dct: <http://purl.org/dct/terms/> .
@prefix ex: <http://aegis-bigdata.eu/examples/> .

ex:All_Trip1_Acceleration

	a aegis:TabularDataSet;
	dct:title "some data";
	dct:description "data bla bla";
	aegis:hasKeyFactor [
		aegis:columnNumber 1;
		aegis:columnHeader "acceleration_id";
		aegis:factorType aegis:NominalKeys ];

	aegis:hasValueFactor
	[	aegis:columnNumber 2;
		aegis:columnHeader "trip_id";
		aegis:factorType aegis:NominalKeys ],
	[	aegis:columnNumber 3;
		aegis:columnHeader "x_value";
		aegis:factorType aegis:Measurements ],
	[	aegis:columnNumber 4;
		aegis:columnHeader "y_value";
		aegis:factorType aegis:Measurements ],
	[	aegis:columnNumber 5;
		aegis:columnHeader "z_value";
		aegis:factorType aegis:Measurements ],
	[	aegis:columnNumber 6;
		aegis:columnHeader "timestamp";
		aegis:factorType aegis:AbsTimes ].
